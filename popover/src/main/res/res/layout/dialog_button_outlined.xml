<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:background="@drawable/outlined_bg"
    tools:gravity="center"
    tools:layout_gravity="center"
    tools:layout_height="44dp"
    tools:layout_marginEnd="@dimen/_32dp"
    tools:layout_marginStart="@dimen/_32dp"
    tools:layout_width="match_parent"
    tools:orientation="horizontal"
    tools:parentTag="androidx.appcompat.widget.LinearLayoutCompat">

    <ImageView
        android:id="@+id/iconView"
        android:layout_width="@dimen/_24dp"
        android:layout_height="@dimen/_24dp"
        app:tint="?iconThemeTint"
        tools:src="@drawable/ic_group_reshare" />

    <TextView
        android:id="@+id/labelView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingHorizontal="@dimen/_12dp"
        android:textColor="@color/blackWhite"
        android:textSize="@dimen/_16sp"
        tools:text="Reshare" />

</merge>
